<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bonss.system.mapper.ModelManualMapper">

    <insert id="batchInsert" parameterType="map">
        INSERT INTO model_manual (model_id, manual_id)
        VALUES
        <foreach collection="manualIds" item="manualId" separator=",">
            (#{id}, #{manualId})
        </foreach>
    </insert>

</mapper>
