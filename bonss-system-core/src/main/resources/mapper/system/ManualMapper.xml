<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bonss.system.mapper.ManualMapper">

    <insert id="insertManualProduct" parameterType="map">
        insert into product_manual (manual_id,product_id)
        values
        <foreach collection="ids" item="item"  separator=",">
        (#{item},#{productId})
        </foreach>
    </insert>

</mapper>
