<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bonss.system.mapper.ModelTutorialMapper">

    <insert id="batchInsert" parameterType="map">
        INSERT INTO model_tutorial (model_id, tutorial_id)
        VALUES
        <foreach collection="tutorialIds" item="tutorialId" separator=",">
            (#{id}, #{tutorialId})
        </foreach>
    </insert>
</mapper>
